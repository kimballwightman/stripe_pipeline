# Stripe API Configuration
STRIPE_API_URL=https://api.stripe.com/v1
STRIPE_API_PUBLISHABLE_KEY=pk_test_your_publishable_key_here
STRIPE_API_SECRET_KEY=sk_test_your_secret_key_here

# GCP Service Account
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json

# DBT BigQuery Profile (used in dbt profiles.yml)
DBT_PROJECT_ID=your-gcp-project-id
DBT_DATASET=stripe_dev  # Use stripe_dev for development, stripe_prod for production
DBT_LOCATION=US  # or EU, etc.
DBT_THREADS=4
DBT_TIMEOUT_SECONDS=300

# Environment Configuration
DBT_TARGET=dev  # dev or prod
ENVIRONMENT=development  # development or production

# Airflow Configuration (optional)
AIRFLOW_UID=50000
AIRFLOW_GID=0

# Other secrets (if needed)
# Add any additional secrets here 